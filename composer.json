{
    "name": "namelivia/wallapoph",
    "description": "A PHP client for wallapop",
    "keywords": ["wallapop"],
    "license": "GLP3",
    "type": "library",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "1.0.*",
        "phpro/grumphp": "^0.14.0",
        "phpunit/phpunit": "~7.0",
        "symfony/var-dumper": "^4.2"
    },
	"autoload": {
		"psr-4": {
			"Namelivia\\Wallapophp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"preferred-install": "dist",
		"minimum-stability": "dev",
		"sort-packages": true
	}
}
